    "# Week 2 Video 1 Task 1\n",
    "class Rectangle:\n",
    "    def __init__(self,length,width):\n",
    "        self.length = length\n",
    "        self.width = width\n",
    "\n",
    "    def calculate_area(self):\n",
    "        return self.length * self.width\n",
    "\n",
    "rectFun = Rectangle(12,25)\n",
    "\n",
    "area = rectFun.calculate_area()\n",
    "print(area)"
